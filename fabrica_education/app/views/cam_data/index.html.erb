<h1>Listing cam_data</h1>

<table>
  <thead>
    <tr>
      <th>Datum</th>
      <th>Name</th>
      <th>Path data</th>
      <th>Slicer</th>
      <th>Print time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cam_data.each do |cam_datum| %>
      <tr>
        <td><%= cam_datum.datum_id %></td>
        <td><%= cam_datum.name %></td>
        <td><%= cam_datum.path_data %></td>
        <td><%= cam_datum.slicer_id %></td>
        <td><%= cam_datum.print_time %></td>
        <td><%= link_to 'Show', cam_datum %></td>
        <td><%= link_to 'Edit', edit_cam_datum_path(cam_datum) %></td>
        <td><%= link_to 'Destroy', cam_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cam datum', new_cam_datum_path %>
