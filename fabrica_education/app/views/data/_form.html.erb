<%= form_for ([@printer, @datum]) do |f| %>
  <% if @datum.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@datum.errors.count, "error") %> prohibited this datum from being saved:</h2>

      <ul>
      <% @datum.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :ファイルの名前 %><br>
    <%= f.text_field :name %>
  </div>
  <p>STLファイルのアップローダーの実装</p>
  <div class="field">
    <%= f.label :cad_data %><br>
    <%= f.text_field :cad_data %>
  </div>
  <div class="field">
    <%= f.label :delete_f %><br>
    <%= f.check_box :delete_f %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
