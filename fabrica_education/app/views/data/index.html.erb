<h1>Listing data</h1>

<table>
  <thead>
    <tr>
      <th>Slice</th>
      <th>User</th>
      <th>Name</th>
      <th>Origin</th>
      <th>Slice</th>
      <th>Print time</th>
      <th>Delete f</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data.each do |datum| %>
      <tr>
        <td><%= datum.slice_id %></td>
        <td><%= datum.user_id %></td>
        <td><%= datum.name %></td>
        <td><%= datum.origin %></td>
        <td><%= datum.slice %></td>
        <td><%= datum.print_time %></td>
        <td><%= datum.delete_f %></td>
        <td><%= link_to 'Show', datum %></td>
        <td><%= link_to 'Edit', edit_datum_path(datum) %></td>
        <td><%= link_to 'Destroy', datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Datum', new_datum_path %>
