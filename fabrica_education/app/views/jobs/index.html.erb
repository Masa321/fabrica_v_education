<h1>Listing jobs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Datum</th>
      <th>Status</th>
      <th>Print time</th>
      <th>Rate</th>
      <th>Photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.user_id %></td>
        <td><%= job.datum_id %></td>
        <td><%= job.status %></td>
        <td><%= job.print_time %></td>
        <td><%= job.rate %></td>
        <td><%= job.photo %></td>
        <td><%= link_to 'Show', job %></td>
        <td><%= link_to 'Edit', edit_job_path(job) %></td>
        <td><%= link_to 'Destroy', job, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Job', new_job_path %>
